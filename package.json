{
  "name": "@edenlabllc/react-components",
  "version": "1.0.0-alpha.0",
  "description": "React Components by Edenlab",
  "main": "lib",
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "lint": "prettier --list-different *.{js,json,md}",
    "docs": "rimraf docs && build-storybook -c .storybook -o docs",
    "build": "rimraf lib && BABEL_ENV=build babel src --out-dir lib",
    "precommit": "lint-staged",
    "prepublish": "npm run build"
  },
  "lint-staged": {
    "*.{js,json,md}": ["prettier --write", "git add"],
    "src/__stories__/**": ["docs", "git add docs"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edenlabllc/react-components.git"
  },
  "keywords": ["components", "react", "edenlab"],
  "author": "Edenlab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edenlabllc/react-components/issues"
  },
  "homepage": "https://github.com/edenlabllc/react-components",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.38",
    "@babel/core": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/preset-react": "^7.0.0-beta.38",
    "@babel/preset-stage-0": "^7.0.0-beta.38",
    "@storybook/addon-actions": "^3.3.10",
    "@storybook/addon-knobs": "^3.3.10",
    "@storybook/addon-options": "^3.3.10",
    "@storybook/react": "^3.3.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.1.0",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "lint-staged": "^6.0.1",
    "prettier": "1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
